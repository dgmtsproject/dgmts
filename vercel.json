{
  "rewrites": [
        {
          "source": "/api/fetchMicromateReadings",
          "destination": "/api/fetchMicromateReadings.ts"
        },
    {
      "source": "/api/(.*)",
      "destination": "https://scs.syscom-instruments.com/public-api/v1/$1"
    },
    {
      "source": "/micromate-api/(.*)",
      "destination": "https://imsite.dullesgeotechnical.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "x-scs-api-key, Content-Type" }
      ]
    }
  ]
}